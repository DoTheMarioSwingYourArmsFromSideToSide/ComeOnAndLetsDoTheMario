	function displayTripDays()
    {
        var id = $("select#itinerary_trip option").filter(":selected").val();
        if (id == 0) {
            $('#trip_days_label').hide();
        } else {
            $('#trip_days_label').show();
        }
        $('.trip-list').hide();
        $('#trip-list-' + id).show();
    }

    function saveItinerary()
    {
        var id = $("select#itinerary_trip option").filter(":selected").val();
        var daysChecked = $('#trip-list-' + id).find('input:checked').length;

        if (id != 0 && daysChecked == 0) {
            jQuery("#itinerarysucessmsg").addClass("error");
            jQuery("p.itinerarymsgshow").html("Please check at least one day.");
            jQuery("#itinerarysucessmsg").show();
        } else {
            jQuery("#itinerarysucessmsg").removeClass("error");
            jQuery("p.itinerarymsgshow").html("");
            jQuery("#itinerarysucessmsg").hide();

            $('#formAddItinerary').hide();
            jQuery.ajax({
                type: "POST",
                url: "index.php?section=addtoitinerary",
                data: $('#formAddItinerary').serialize(),
                success: function (result) {
                    if(result == 1)
                    {
                        jQuery("#itinerarysucessmsg").removeClass("error");
                        jQuery("p.itinerarymsgshow").html("Successfully added to your itinerary.");
                        jQuery("#itinerarysucessmsg").show();
                        jQuery("#showitineraryresponsepopup").show();
                        setTimeout(function(){ jQuery("#showitineraryresponsepopup").hide() }, 4000);
                        jQuery("#pagosa-placeholder-image").hide();


                    }
                    else if(result == 3)
                    {
                        jQuery("#itinerarysucessmsg").addClass("error");
                        jQuery("p.itinerarymsgshow").html("This item is already added in your itinerary.");
                        jQuery("#itinerarysucessmsg").show();
                        jQuery("#showitineraryresponsepopup").show();
                        setTimeout(function(){ jQuery("#showitineraryresponsepopup").hide() }, 4000);
                        jQuery("#pagosa-placeholder-image").hide();

                    }
                    else
                    {
                        jQuery("#itinerarysucessmsg").addClass("error");
                        jQuery("p.itinerarymsgshow").html("Failed added to your itinerary.");
                        jQuery("#itinerarysucessmsg").show();
                        jQuery("#showitineraryresponsepopup").show();
                        setTimeout(function(){ jQuery("#showitineraryresponsepopup").hide() }, 4000);
                        jQuery("#pagosa-placeholder-image").hide();

                    }
                }
            });
        }
    }
	

