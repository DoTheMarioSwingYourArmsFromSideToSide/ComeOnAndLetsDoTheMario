function checkOS(link){
	var screensaver = "m/06/chevyracing/screensavers/chevy_winning_moments";
	if(isMac)
		screensaver += "_macx.zip"
	else if (isWin)
		screensaver += "_pc.zip"
	else {
		link.rel = link.rel.replace("Screensaver","Feature"); 
		newWindow.setURL(link);
		return false;
	}
	link.href = screensaver;
	return true;
}

