/**
 * JS for the handling of the the regions component.
 */

(function ($, Drupal, window, document, undefined) {
  'use strict';

  let selectCounter = 0;

  // To understand behaviors, see https://drupal.org/node/756722#behaviors
  Drupal.behaviors.arkRegionsMap = {
    attach: function (context, settings) {
      $('.block--regions-map', context)
        .once('regions-map-initialized')
        .each(function () {
          const $root = $(this);
          const $svg = $root.find('svg#regions-map-svg');
          let svgClickSemaphore = false;

          // detect rollover, add class to corresponding views-row
          $svg.find('.regions-map-svg--area, .regions-map-svg--text', this).click(function (e) {
            e.preventDefault();

            // Prevents an infinite loop when updating Chosen.
            if (svgClickSemaphore) {
              return;
            }

            svgClickSemaphore = true;
            let regionName = $(this).attr('data-region');

            // Process/add/remove classes
            $('.regions-map-svg--area, .regions-map-svg--text', $root).removeClass('clicked');
            $svg.find(`[data-region='${regionName}']`, $root).addClass('clicked');

            $('.block--regions-map--info .region-info', $root)
              .removeClass('active')
              .filter('.region-' + regionName)
              .addClass('active');

            $(this).addClass('clicked');

            // Update the select
            $root.find('select.regions-map-filter').each(function () {
              $(this).find('option')
                .attr('selected', null)
                .filter(`[value=${ regionName }]`)
                .attr('selected', 'selected');

              $(this).trigger('chosen:updated');
            });

            svgClickSemaphore = false;
          });

          // Create the region dropdown.
          $root.find('.regions-map-filter-stub')
            .once('regions-map-filter-initialized')
            .each(function () {
              let active = '';
              let region_change_map = {};

              // Build the dropdown
              let wid = 'regions-map-filter';
              if (selectCounter++ > 0) {
                wid += '-' + selectCounter;
              }

              const $filter_widget = $(`<select id="${wid}" class="regions-map-filter"></select>`);

              $root.find('.region-info')
                .each(function () {
                  const label = $(this).find('h3').text().trimLeft().trimRight();
                  const classes = $(this).attr('class').split(' ');
                  let region_class = '';

                  for (let c of classes) {
                    if (c !== 'region-info' && c.match(/^region-/)) {
                      region_class = c;
                      const $option = $(`<option value="${c}">${label}</option>`);

                      if ($(this).hasClass('active')) {
                        $option.attr('selected', 'selected');
                        active = c;
                      }

                      $filter_widget.append($option);
                      break;
                    }
                  }

                  region_change_map[region_class] = $svg.find(`#${region_class}`);
                });

              $(this)
                .empty()
                .removeClass('regions-map-filter-stub')
                .addClass('regions-map-filter')
                .append($filter_widget);

              if (active === '') {
                active = 'northwest';
                $root.find('select.regions-map-filter option[value=northwest]')
                  .attr('selected', 'selected');
              }

              // Attach Chosen.
              $root.find('select.regions-map-filter')
                .chosen({})
                .on('change', function (e, params) {
                  region_change_map[params.selected].click();
                });
            });
        });
    }
  };
})(jQuery, Drupal, this, this.document); // jshint ignore:line
